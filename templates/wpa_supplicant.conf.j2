ctrl_interface=/run/wpa_supplicant
ctrl_interface_group=wheel
update_config=1
country=US

{% for wpa_passphrase_result in (wpa_passphrase_results.results | default([]) | reject("skipped") | list) %}
{{ wpa_passphrase_result.stdout }}
{% endfor %}
{% for network in (networking.networks | default([]) | rejectattr("password", "defined") | list) %}
network={
        ssid="{{ network.ssid }}"
        key_mgmt=NONE
}
{% endfor %}
