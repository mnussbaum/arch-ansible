{
  "layer": "top",
  "modules-left": [
    "sway/workspaces",
    "sway/mode"
  ],
  "modules-right": [
    "battery",
    "cpu",
    "memory",
    "network",
    "pulseaudio",
    "clock",
    "tray"
  ],
  "cpu": {
    "format": "{usage}% "
  },
  "memory": {
    "format": "{}% "
  },
  "network": {
    "format-wifi": "{essid} {signalStrength}% ",
    "format-ethernet": "{ifname}: {ipaddr}/{cidr} ",
    "format-disconnected": "Disconnected ⚠"
  },
  "pulseaudio": {
    "format": "{volume}% ",
    "format-muted": "0% ",
    "on-click": "pavucontrol"
  },
  "battery": {
    "format-charging": "{capacity}% ",
    "format": "{capacity}% {icon}",
    "format-icons": [
      "",
      "",
      "",
      "",
      ""
    ],
    "states": {
      "warning": 30,
      "critical": 20
    }
  },
  "clock": {
    "format-alt": "{:%a, %d %b %H:%M:%S}"
  },
  "tray": {
    "spacing": 10
  }
}
